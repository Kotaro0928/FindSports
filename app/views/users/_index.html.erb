<table class="table table-borderless table-sm">
  <% users.each do |user| %>
    <tr>
      <td>
        <%= link_to user_path(user), class:'user_name' do %>
          <%= attachment_image_tag user, :profile_image, size: '30x30', format:'jpg', fallback:'no_image.png', class:'profile_image' %>
          <%= user.name %></br>
        <% end %>
      </td>
    </tr>
  <% end %>
</table>
<div class="page">
  <%= paginate users %>
</div>